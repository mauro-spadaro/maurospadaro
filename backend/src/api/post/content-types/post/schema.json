{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "PublishedDate": {
      "type": "date",
      "required": true
    },
    "ReadingTime": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title",
      "required": true
    },
    "Summary": {
      "type": "string",
      "required": true
    },
    "Body": {
      "type": "blocks"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "mappedBy": "posts"
    },
    "Thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
